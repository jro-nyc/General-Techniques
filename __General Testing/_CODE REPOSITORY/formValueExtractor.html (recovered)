<script>
//Get the form values for default stuff
function formInputs(frm,el){
	this.name = el.name;
	this.value = el.value;
	this.type = el.type;
	this.formName = frm.name;
}	
function formCheckInputs(frm,el,check){
	this.name = el.name;
	this.value = check;
	this.type = el.type;
	this.formName = frm.name;
}	
function doFormStuff(){
	var daFormElements =  new Object;
	var formProps =  new Object;
	formObj = document.forms[0];	
	daFormElements[formObj.name] = new Object;
	list='';
	for (i=0;i<formObj.length;i++) {
		if(typeof(formObj.elements[i].type)!="undefined"){	//Found an instance of fieldsets and labels seen as form elements, filter them out.
			if(formObj.elements[i].type.toLowerCase().indexOf('radio')== -1 && formObj.elements[i].type.toLowerCase().indexOf('checkbox')== -1) {
				daFormElements[formObj.name][i] = new formInputs(formObj,formObj.elements[i]);
				list += 'The name is ' + formObj.elements[i].name + ' which has a type of ' + formObj.elements[i].type + ' and a value of ' + formObj.elements[i].value+ '\n';
			}
			else if(formObj.elements[i].type.toLowerCase().indexOf('checkbox')== -1){
				if(formObj.elements[i].checked == true) {
					daFormElements[formObj.name][i] = new formInputs(formObj,formObj.elements[i],formObj.elements[i].value);
					list += 'The name is ' + formObj.elements[i].name + ' which has a type of ' + formObj.elements[i].type + ' and a value of ' + formObj.elements[i].value + '\n';
				}
			}
			else {
				if(formObj.elements[i].checked == true){
					daFormElements[formObj.name][i] = new formCheckInputs(formObj,formObj.elements[i],true);
					list += 'The name is ' + formObj.elements[i].name + ' which has a type of ' + formObj.elements[i].type + ' and a value of ' + formObj.elements[i].checked+ '\n';
				}
				else{
					daFormElements[formObj.name][i] = new formCheckInputs(formObj,formObj.elements[i],false);
					list += 'The name is ' + formObj.elements[i].name + ' which has a type of ' + formObj.elements[i].type + ' and a value of ' + formObj.elements[i].checked+ '\n';
				}
			}	
		}		
	}
	alert(list);
	list='';
	//for (i=0;i<daFormElements[formObj.name].length;i++) {
	for(var i in daFormElements[formObj.name]){
		list+=daFormElements[formObj.name][i].name+'='+daFormElements[formObj.name][i].value+'\n';
	}
	alert(list);
}
doFormStuff();
</script>
